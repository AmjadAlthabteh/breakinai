  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/BreakIn.ai.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/BreakIn.ai/issues"
  },
  "homepage": "https://github.com/yourusername/BreakIn.ai#readme",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
