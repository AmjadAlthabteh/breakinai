{
  "name": "breakin-ai",
  "version": "1.0.0",
  "description": "Career Hub & Resume Optimization Platform",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/server.ts",
    "start": "node dist/server.js",
    "watch": "tsc -w",
    "clean": "rm -rf dist",
    "rebuild": "npm run clean && npm run build",
    "type-check": "tsc --noEmit",
    "example": "ts-node src/example.ts"
  },
  "keywords": [
    "career",
